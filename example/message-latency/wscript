# Waf build script for an RTEMS Hello
import rtems_waf.rtems as rtems

def build(bld):
    rtems.build(bld)
    bld.includes = ['../support']
    bld(features = 'c cprogram',
        target = 'message-latency.exe',
        includes = bld.includes,
        source = ['message-latency.c'])
